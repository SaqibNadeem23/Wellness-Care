                </StackLayout>
                <StackLayout  WidthRequest="150">
                    <Label Text="InsulinService"></Label>
                </StackLayout>
                <StackLayout  WidthRequest="150" >
                    <Label Text="PainKillerService"></Label>
                </StackLayout>
                <StackLayout  WidthRequest="150">
                    <Label Text="ChestPainService"></Label>
                </StackLayout>
                <StackLayout  WidthRequest="150" >
                    <Label Text="MinorInjuryService"></Label>
                </StackLayout>
                <StackLayout  WidthRequest="150">
                    <Label Text="BreathingProblemService"></Label>
                </StackLayout>
            </StackLayout>

        </StackLayout>
    </ScrollView>
</ContentPage>